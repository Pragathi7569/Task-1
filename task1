#List Operations
#Creating a list
my_list=[10, 20, 30, 40, 50]
print("Original List",my_list)
#Add an element
my_list.append(60)
print("List after appending Black:",my_list)
#remove an element
my_list.remove(40)
print("List after removing yellow:",my_list)
#Modify an element
my_list[2]= 80
print("List after modifying the third element to 80:",my_list)


#Dictionary Operations
#creating an Dictionary
my_dict = {'name': 'Akshay', 'age': 20, 'city': 'Delhi'}
print("Original dict:",my_dict)
#Add
my_dict['Job'] = 'Doctor'
print("Dictionary after adding:",my_dict)
#Remove
my_dict.pop('city')
print("Dictionary after removing:",my_dict)
#Modify
my_dict['age'] = 24
print("Dictionary after modifying the age:",my_dict)
#Set Operations
#Creating a Set
my_set = {1, 2, 3, 4, 5}
print("Original set",my_set)
#Add
my_set.add(6)
print("Set after adding:",my_set)
#Remove
my_set.remove(3)
print("Set after removing:",my_set)
#Modify
my_set.update([7, 8])
print("Set after modifying:",my_set)

